            "text": "Einfach nur okay", 
            "relative_time_description": "3 months ago", 
            "author_url": "https://www.google.com/maps/contrib/104848967601713645771/reviews", 
            "time": 1552977923, 
            "profile_photo_url": "https://lh4.googleusercontent.com/-gDhrbwbKRfw/AAAAAAAAAAI/AAAAAAAAAAA/ACHi3reM1womKMa0semHVJ8RGLdMlBY3Hw/s128-c0x00000000-cc-rp-mo-ba4/photo.jpg"
